{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": ["prettier", "import"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "16.0"
    }
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": 0
  }
}
